<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="io.corber.anybit" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>anybit</name>
    <description>
    </description>
    <content src="index.html"/>
    <plugin name="cordova-plugin-whitelist" spec="1"/>
    <plugin name="cordova-plugin-wkwebview-engine" spec="~1.1.4"/>
    <plugin name="cordova-plugin-secure-storage" spec="^3.0.1"/>
    <plugin name="cordova-plugin-qrscanner" spec="^2.6.0"/>
    <access origin="*"/>
    <allow-intent href="http://*/*"/>
    <allow-intent href="https://*/*"/>
    <allow-intent href="tel:*"/>
    <allow-intent href="sms:*"/>
    <allow-intent href="mailto:*"/>
    <allow-intent href="geo:*"/>
    <platform name="android">
        <allow-intent href="market:*"/>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*"/>
        <allow-intent href="itms-apps:*"/>
    </platform>
    <engine name="android" spec="~7.0.0"/>
    <engine name="ios" spec="~4.5.5"/>
</widget>